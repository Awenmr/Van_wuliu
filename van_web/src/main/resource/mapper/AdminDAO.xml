<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.van.mapper.AdminDAO">

    <resultMap type="Admin" id="adminMap">
        <id property="adminId" column="admin_id"/>
        <result property="adminPwd" column="admin_pwd"/>
    </resultMap>

    <select id="login" resultMap="adminMap" parameterType="com.van.moudle.Admin">
			select * from admin where admin_id=#{adminId} and  admin_pwd=#{adminPwd}
	</select>


</mapper>
